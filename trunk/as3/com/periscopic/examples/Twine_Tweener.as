package com.periscopic.examples {			import com.periscopic.utils.Twine;		import caurina.transitions.Tweener;	import caurina.transitions.Equations;		import flash.display.Sprite;	import flash.utils.setTimeout;	import flash.geom.Point;			public class Twine_Tweener extends Sprite {				private var p:Point;		private var p2:Point;		private var _step:Number=0.0;		private var _useMix:Boolean=false;		public function Twine_Tweener() {			// constructor code						graphics.lineStyle(1,0,0.1);			graphics.drawRect(0,0,225,225);			setTimeout(go,2000);		}				private function go():void{			if(_step>=0.9)				return;			_step+=0.1;			p = new Point(0,0);			p2 = new Point(0,0);			graphics.moveTo(1,224);			graphics.lineStyle(1,0);						if(_useMix){				var mixParams:Object=Twine.mixParams(Equations.easeNone,Equations.easeOutQuint,_step);				Tweener.addTween(p,{time:2,x:223,transition:Equations.easeNone});				Tweener.addTween(p2,{time:2,y:223,transition:Twine.mix,transitionParams:mixParams,onUpdate:render,onComplete:this.go});			}else{				var spliceParams:Object=Twine.spliceParams(Equations.easeInQuart,Equations.easeOutQuad,_step,false);				Tweener.addTween(p,{time:2,x:223,transition:Equations.easeNone});				Tweener.addTween(p2,{time:2,y:223,onUpdate:render,transition:Twine.splice,transitionParams:spliceParams,onComplete:this.go});						}		}		private function render():void{			graphics.lineTo(p.x+1,224-p2.y);				}	}	}