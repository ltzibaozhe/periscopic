package  com.periscopic.examples{		import com.periscopic.utils.Twine;		import com.greensock.TweenLite;		import com.greensock.easing.Linear;	import com.greensock.easing.Quad;	import com.greensock.easing.Quart;	import flash.display.Sprite;	import flash.utils.setTimeout;	import flash.geom.Point;	import com.greensock.easing.Quint;		public class Twine_TweenLite extends Sprite {				private var p:Point;		private var p2:Point;		private var _step:Number=0.0;		private var _useMix:Boolean=false;		public function Twine_TweenLite() {			// constructor code						graphics.lineStyle(1,0,0.1);			graphics.drawRect(0,0,225,225);			setTimeout(go,2000);		}				private function go():void{			if(_step>=0.9)				return;			_step+=0.1;			p = new Point(0,0);			p2 = new Point(0,0);			graphics.moveTo(1,224);			graphics.lineStyle(1,0);						if(_useMix){				var mixParams:Object=Twine.mixParams(Linear.easeNone,Quint.easeOut,_step);				TweenLite.to(p,2,{x:223,ease:Linear.easeNone});				TweenLite.to(p2,2,{y:223,ease:Twine.mix,easeParams:mixParams,onUpdate:render,onComplete:this.go});			}else{				var spliceParams:Object=Twine.spliceParams(Quart.easeIn,Quad.easeOut,_step,false);				TweenLite.to(p,2,{x:223,ease:Linear.easeNone});				TweenLite.to(p2,2,{y:223,onUpdate:render,ease:Twine.splice,easeParams:spliceParams,onComplete:this.go});						}		}		private function render():void{			graphics.lineTo(p.x+1,224-p2.y);				}	}	}